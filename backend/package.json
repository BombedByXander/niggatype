{
  "name": "@monkeytype/backend",
  "version": "1.0.0",
  "description": "Monkeytype backend server",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint --type-aware --type-check",
    "lint-fix": "oxlint --fix",
    "docker": "docker build -t monkeytype-backend .",
    "docker:dev": "docker-compose up --build"
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "cors": "2.8.5",
    "cron": "4.1.0",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "ioredis": "5.4.1",
    "mjml": "4.14.1",
    "mustache": "4.2.0",
    "nodemailer": "6.9.15",
    "object-hash": "3.0.0",
    "openapi3-ts": "2.0.2",
    "string-similarity": "4.0.4",
    "swagger-stats": "0.95.11",
    "ua-parser-js": "2.0.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@monkeytype/oxlint-config": "workspace:*",
    "@monkeytype/typescript-config": "workspace:*",
    "@redocly/cli": "2.0.5",
    "@types/bcrypt": "5.0.2",
    "@types/cors": "2.8.12",
    "@types/cron": "1.7.3",
    "@types/express": "5.0.3",
    "@types/ioredis": "4.28.10",
    "@types/mjml": "4.7.4",
    "@types/mustache": "4.2.2",
    "@types/node": "24.9.1",
    "@types/nodemailer": "6.4.15",
    "@types/object-hash": "3.0.6",
    "@types/readline-sync": "1.4.8",
    "@types/string-similarity": "4.0.2",
    "@types/supertest": "6.0.3",
    "@types/swagger-stats": "0.95.11",
    "@types/ua-parser-js": "0.7.36",
    "@types/uuid": "10.0.0",
    "@vitest/coverage-v8": "4.0.15",
    "concurrently": "8.2.2",
    "openapi3-ts": "2.0.2",
    "oxlint": "1.50.0",
    "oxlint-tsgolint": "0.14.2",
    "readline-sync": "1.4.10",
    "supertest": "7.1.4",
    "testcontainers": "11.11.0",
    "tsx": "4.16.2",
    "typescript": "6.0.0-beta",
    "vitest": "4.0.15"
  }
}
